#!/usr/bin/env bash

brew update && brew upgrade && brew cleanup && brew prune
brew upgrade brew-cask && brew cask cleanup
brew outdated

gem update --system && gem update && gem clean
gem cleanup
rvm cleanup all

# Update RVM installation
rvm get stable
rvmGemsetsCleanup

# Custom cleaners
resetFinderOpenWith
resetLaunchpadDB

# Get latest podspecs
pod repo update

